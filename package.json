{
    "name": "ng-scala-play",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "build": "ng build",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "start": "ng serve --open --host 0.0.0.0 --disable-host-check",
        "test": "ng test --watch",
        "test:ci": "ng test --watch=false --browsers ChromeHeadless",
        "test:coverage": "ng test --watch=true --code-coverage=true",
        "test:coverage:ci": "ng test --watch=false --code-coverage=true --browsers ChromeHeadless",
        "build:dev": "ng build --progress --output-path ../public",
        "build:prod": "ng build --progress --prod --output-path ../public"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~11.2.13",
        "@angular/cdk": "^11.2.12",
        "@angular/common": "~11.2.13",
        "@angular/compiler": "~11.2.13",
        "@angular/core": "~11.2.13",
        "@angular/forms": "~11.2.13",
        "@angular/material": "^11.2.12",
        "@angular/platform-browser": "~11.2.13",
        "@angular/platform-browser-dynamic": "~11.2.13",
        "@angular/router": "~11.2.13",
        "@auth0/auth0-angular": "^1.5.0",
        "class-transformer": "^0.4.0",
        "https": "^1.0.0",
        "https-localhost": "^4.6.5",
        "ngx-cookie-service": "^12.0.0",
        "reflect-metadata": "^0.1.13",
        "rxjs": "~6.6.0",
        "tslib": "^2.0.0",
        "zone.js": "~0.11.3"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.1102.12",
        "@angular/cli": "~11.2.12",
